{
	"name": "diagman-api",
	"version": "1.2.0",
	"private": true,
	"scripts": {
		"deploy": "wrangler deploy --name diagman-api-dev --var TURSO_DB_URL:libsql://diagman-opinion-dev-permafrost06.turso.io",
		"deploy:prod": "wrangler deploy",
		"start": "wrangler dev",
		"db:generate": "drizzle-kit generate:sqlite",
		"db:push": "node ./database/migrate.js",
		"db:migrate": "drizzle-kit generate:sqlite && node ./database/migrate.js",
		"db:drop": "drizzle-kit drop",
		"cli": "node ./cli/main.js"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20240712.0",
		"@inquirer/prompts": "^3.3.2",
		"@types/bcryptjs": "^2.4.6",
		"cli-spinner": "^0.2.10",
		"dotenv": "^16.4.5",
		"drizzle-kit": "^0.19.13",
		"drizzle-orm": "^0.28.6",
		"typescript": "^5.5.3",
		"wrangler": "^3.64.0"
	},
	"dependencies": {
		"@faker-js/faker": "^8.4.1",
		"@libsql/client": "^0.3.6",
		"bcryptjs": "^2.4.3",
		"itty-router": "^4.2.2",
		"zod": "^3.23.8"
	}
}
